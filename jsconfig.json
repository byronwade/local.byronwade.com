{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@components/*": ["./src/components/*"],
			"@features/*": ["./src/features/*"],
			"@integrations/*": ["./src/integrations/*"],
			"@utils": ["./src/utils"],
			"@utils/*": ["./src/utils/*"],
			"@lib/*": ["./src/lib/*"],
			"@types/*": ["./src/types/*"],
			"@context/*": ["./src/context/*"],
			"@hooks/*": ["./src/hooks/*"],
			"@store/*": ["./src/store/*"],
			"@data/*": ["./src/data/*"],
			"@config": ["./src/config/index.js"],
			"@config/*": ["./src/config/*"]
		}
	},
	"exclude": ["node_modules", ".next", "apps"],
	"include": ["**/*.js", "**/*.jsx", "**/*.ts", "**/*.tsx"]
}